{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source" : "*",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=300"
      }]
    }],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
